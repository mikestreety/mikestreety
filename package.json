{
  "name": "mikestreety",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prod:11ty": "npx @11ty/eleventy",
    "watch:11ty": "npx @11ty/eleventy --serve",
    "prod:assets": "mix --production",
    "watch:assets": "mix watch",
    "watch": "tinacms dev -c \"npm run dev\"",
    "dev": "npm run datacollector:collect && npm run watch:assets & npm run watch:11ty",
    "prod": "tinacms build && npm run prod:assets && npm run prod:11ty",
    "build": "npm run prod",
    "datacollector:collect": "npm run dc:webmentions && npm run dc:letterboxd",
    "dc:webmentions": "datacollector webmentions app/data/webmentions.json -d www.mikestreety.co.uk -t lId0N38z8IMrDHih2VFZ7Q",
    "dc:letterboxd": "datacollector letterboxd app/data/letterboxd.json -u mikestreety"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@11tyrocks/eleventy-plugin-emoji-readtime": "^1.0.1",
    "@mikestreety/11ty-utils": "^1.3",
    "@mikestreety/data-collector": "^0.2.0",
    "@tinacms/cli": "^1.5.11",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "html-to-text": "^9.0.5",
    "laravel-mix": "^6.0.49",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.1",
    "resolve-url-loader": "^5.0.0",
    "sanitize-html": "^2.10.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "tinacms": "^1.5.4"
  }
}
